# Silentlake
/dev/vidt                   u:object_r:silentlake_device:s0
/system/bin/sl_si_service   u:object_r:silentlake_exec:s0
/data/silent_lake(/.*)?     u:object_r:silentlake_data_file:s0
/dev/socket/silentlake      u:object_r:silentlake_socket:s0

# thermal management
/dev/socket/logconfig                  u:object_r:thermal_socket:s0
/sbin/thermald                         u:object_r:thermal_exec:s0

# ITUX
/sys/devices/pci.*/.*/drm/card0/gt_.*    u:object_r:sysfs_thermal_management:s0

# bcu_cpufreqrel
/system/bin/bcu_cpufreqrel      u:object_r:bcu_cpufreqrel_exec:s0

# Fuel Gauge userspace helper
/sbin/fg_algo_cos                                      u:object_r:fg_algo_cos_exec:s0
